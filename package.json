{
	"name": "better-touch-bar",
	"displayName": "Better Touch Bar",
	"description": "Adds common functions to the touch bar",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "better-touch-bar.tabNav.previous",
				"title": "1-Previous Tab",
				"icon": "./assets/back.png"
			},
			{
				"command": "better-touch-bar.tabNav.next",
				"title": "2-Next Tab",
				"icon": "./assets/forward.png"
			},
			{
				"command": "better-touch-bar.cancel",
				"title": "Cancel"
			},
			{
				"command": "better-touch-bar.run.main",
				"title": "Run",
				"icon": "./assets/run.png"
			},
			{
				"command": "better-touch-bar.run.build",
				"title": "Build"
			},
			{
				"command": "better-touch-bar.run.deploy",
				"title": "Deploy"
			},
			{
				"command": "better-touch-bar.run.startRioLog",
				"title": "RioLog"
			},
			{
				"command": "better-touch-bar.run.simulate",
				"title": "Simulate"
			},
			{
				"command": "better-touch-bar.run.startTool",
				"title": "Start Tool"
			},
			{
				"command": "better-touch-bar.git.display",
				"title": "1-Display",
				"icon": "./assets/git.png"
			},
			{
				"command": "better-touch-bar.git.branch",
				"title": "Branch..."
			},
			{
				"command": "better-touch-bar.git.checkout",
				"title": "Checkout"
			},
			{
				"command": "better-touch-bar.git.merge",
				"title": "Merge"
			},
			{
				"command": "better-touch-bar.git.reset",
				"title": "Reset"
			},
			{
				"command": "better-touch-bar.git.pop",
				"title": "Pop Stash"
			},
			{
				"command": "better-touch-bar.git.stash",
				"title": "Stash"
			},
			{
				"command": "better-touch-bar.git.sync",
				"title": "Sync"
			},
			{
				"command": "better-touch-bar.dev.rename",
				"title": "üìù"
			},
			{
				"command": "better-touch-bar.dev.peek",
				"title": "üîç"
			},
			{
				"command": "better-touch-bar.dev.peekDefinition",
				"title": "Definition"
			},
			{
				"command": "better-touch-bar.dev.peekTypeDefinition",
				"title": "Type Def"
			},
			{
				"command": "better-touch-bar.dev.peekImplementations",
				"title": "Implements"
			},
			{
				"command": "better-touch-bar.dev.peekReferences",
				"title": "References"
			},
			{
				"command": "better-touch-bar.dev.quickFix",
				"title": "üõ†"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "better-touch-bar.tabNav.previous",
					"when": "false"
				},
				{
					"command": "better-touch-bar.tabNav.next",
					"when": "false"
				},
				{
					"command": "better-touch-bar.cancel",
					"when": "false"
				},
				{
					"command": "better-touch-bar.run.main",
					"when": "false"
				},
				{
					"command": "better-touch-bar.run.build",
					"when": "false"
				},
				{
					"command": "better-touch-bar.run.deploy",
					"when": "false"
				},
				{
					"command": "better-touch-bar.run.startRioLog",
					"when": "false"
				},
				{
					"command": "better-touch-bar.run.simulate",
					"when": "false"
				},
				{
					"command": "better-touch-bar.run.startTool",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.display",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.branch",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.checkout",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.reset",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.merge",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.pop",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.stash",
					"when": "false"
				},
				{
					"command": "better-touch-bar.git.sync",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.rename",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.peek",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.peekDefinition",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.peekTypeDefinition",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.peekImplementations",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.peekReferences",
					"when": "false"
				},
				{
					"command": "better-touch-bar.dev.quickFix",
					"when": "false"
				}
			],
			"touchBar": [
				{
					"command": "better-touch-bar.tabNav.previous",
					"group": "navigation"
				},
				{
					"command": "better-touch-bar.tabNav.next",
					"group": "navigation"
				},
				{
					"command": "better-touch-bar.cancel",
					"group": "cancel",
					"when": "better-touch-bar:menu == 'run' || better-touch-bar:menu == 'git' || better-touch-bar:menu == 'peek'"
				},
				{
					"command": "better-touch-bar.run.main",
					"group": "group_1",
					"when": "better-touch-bar:menu == 'main' && !isDebugMode"
				},
				{
					"command": "better-touch-bar.run.build",
					"group": "run_group_1",
					"when": "better-touch-bar:menu == 'run'"
				},
				{
					"command": "better-touch-bar.run.deploy",
					"group": "run_group_1",
					"when": "better-touch-bar:menu == 'run'"
				},
				{
					"command": "better-touch-bar.run.startRioLog",
					"group": "run_group_1",
					"when": "better-touch-bar:menu == 'run'"
				},
				{
					"command": "better-touch-bar.run.simulate",
					"group": "run_group_1",
					"when": "better-touch-bar:menu == 'run'"
				},
				{
					"command": "better-touch-bar.run.startTool",
					"group": "run_group_1",
					"when": "better-touch-bar:menu == 'run'"
				},
				{
					"command": "better-touch-bar.git.display",
					"group": "group_2",
					"when": "better-touch-bar:menu == 'main'"
				},
				{
					"command": "better-touch-bar.git.branch",
					"group": "group_2",
					"when": "better-touch-bar:menu == 'main'"
				},
				{
					"command": "better-touch-bar.git.checkout",
					"group": "git_group_1",
					"when": "better-touch-bar:menu == 'git'"
				},
				{
					"command": "better-touch-bar.git.merge",
					"group": "git_group_1",
					"when": "better-touch-bar:menu == 'git'"
				},
				{
					"command": "better-touch-bar.git.reset",
					"group": "git_group_1",
					"when": "better-touch-bar:menu == 'git'"
				},
				{
					"command": "better-touch-bar.git.pop",
					"group": "git_group_2",
					"when": "better-touch-bar:menu == 'git'"
				},
				{
					"command": "better-touch-bar.git.stash",
					"group": "git_group_2",
					"when": "better-touch-bar:menu == 'git'"
				},
				{
					"command": "better-touch-bar.git.sync",
					"group": "group_2",
					"when": "better-touch-bar:menu == 'main'"
				},
				{
					"command": "better-touch-bar.dev.rename",
					"group": "group_3",
					"when": "better-touch-bar:menu == 'main'"
				},
				{
					"command": "better-touch-bar.dev.peek",
					"group": "group_3",
					"when": "better-touch-bar:menu == 'main'"
				},
				{
					"command": "better-touch-bar.dev.peekDefinition",
					"group": "peek_group_1",
					"when": "better-touch-bar:menu == 'peek'"
				},
				{
					"command": "better-touch-bar.dev.peekTypeDefinition",
					"group": "peek_group_1",
					"when": "better-touch-bar:menu == 'peek'"
				},
				{
					"command": "better-touch-bar.dev.peekImplementations",
					"group": "peek_group_2",
					"when": "better-touch-bar:menu == 'peek'"
				},
				{
					"command": "better-touch-bar.dev.peekReferences",
					"group": "peek_group_2",
					"when": "better-touch-bar:menu == 'peek'"
				},
				{
					"command": "better-touch-bar.dev.quickFix",
					"group": "group_3",
					"when": "better-touch-bar:menu == 'main'"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}